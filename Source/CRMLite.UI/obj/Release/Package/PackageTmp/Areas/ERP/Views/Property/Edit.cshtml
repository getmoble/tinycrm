                                    <thead>
                                        <tr>
                                            <th scope="col">Sl No</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Email</th>
                                            <th scope="col">Mobile</th>
                                            <th scope="col">Ownership %</th>
                                            <th scope="col">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="template: { name: 'list-Owners-template',foreach:$root.owners }"></tbody>
                                </table>
                            </div>
                        </div>
                            </div>
                        <div class="panel panel-default">
                            <button type="button" class="btn bg-light text-left col-md-12 top5" data-toggle="collapse" data-target="#Manager"> Manager <span class="fa fa-chevron-down pull-right"></span> </button>
