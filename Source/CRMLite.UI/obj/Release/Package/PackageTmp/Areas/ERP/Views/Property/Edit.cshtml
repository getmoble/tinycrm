    <div class="modal fade" id="createAminityForProperty" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                </div>
                <div class="modal-body clearfix" data-bind="with:selectedAminity">
                    <h4 class="text-primary">Add Amenity</h4>
                    <div class="col-lg-12">
                        <div class="form-group ">
                            <label>Amenity Name</label>
                            <input type="text" data-bind="value:Name" class="form-control" />
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="modal-footer">
                    <div class="col-md-12">
                        <button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">Cancel</button>
                        <button type="button" class="btn btn-primary" data-bind="click:$root.CreateAminity">Save</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="createOwnership" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <strong>Owner List</strong>
                </div>
                <div class="modal-body clearfix">
                    <h4 class="text-primary">Add Owners</h4>
                    <table class="table table-striped table-bordered table-responsive table-hover responsive-table" data-toggle="table" id="ownerDatatable" data-url="data2.json" data-pagination="true" data-search="true">
                        <thead>
                            <tr>
                                <th scope="col"></th>
                                <th scope="col">Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Mobile</th>
                                <th scope="col">Ownership %</th>
                            </tr>
                        </thead>
                        <tbody data-bind="template: { name: 'edit-owner-template',foreach:$root.ownerLists }"></tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <div class="col-md-12">
                        <button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">
                            Cancel
                        </button>

                        <button type="button" class="btn btn-primary" data-dismiss="modal" data-bind="click:$root.addOwners" aria-hidden="true">
                            Add
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="createUnits" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <strong>Units</strong>
                </div>
                <div class="modal-body clearfix">
                    <h4 class="text-primary">Add Units</h4>

                    <div class="scroll-x">
                        <table class="table table-striped table-bordered table-responsive table-hover responsive-table" data-toggle="table" id="unitDatatable" data-url="data2.json" data-pagination="true" data-search="true">
                            <thead class="bg-grey">
                                <tr>
                                    <th scope="col"></th>
                                    <th scope="col">Unit Type</th>
                                    <th scope="col">Property</th>
                                    <th scope="col">Category</th>
                                    <th scope="col">Total Area</th>
                                    <th scope="col">Furnish Type</th>
                                </tr>
                            </thead>
                            <tbody data-bind="template: { name: 'list-unit-template',foreach:$root.unitLists }"></tbody>

                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="col-md-12">
                        <button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">
                            Cancel
                        </button>

                        <button type="button" class="btn btn-primary" data-dismiss="modal" data-bind="click:$root.addUnits" aria-hidden="true">
                            Add
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="createManagers" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-primary">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <strong>Manager Lists</strong>
                </div>
                <div class="modal-body clearfix">
                    <h4 class="text-primary">Add Manager</h4>
                    <table class="table table-striped table-bordered table-responsive table-hover responsive-table" data-toggle="table" id="addManagerDataTable" data-url="data2.json" data-pagination="true" data-search="true">
                        <thead class="bg-grey">
                            <tr>
                                <th scope="col"></th>
                                <th scope="col">Sl No</th>
                                <th scope="col">Role</th>
                                <th scope="col">Name</th>
                                <th scope="col">Email</th>
                                <th scope="col">Mobile</th>
                            </tr>
                        </thead>
                        <tbody data-bind="template: { name: 'list-manager-template',foreach:$root.managerList }"></tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <div class="col-md-12">
                        <button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">
                            Cancel
                        </button>
                        <button class="btn btn-primary ladda-button" data-style="expand-left" data-bind="ladda:$root.busy,click:$root.addManager"><span class=" ladda-label">Add</span></button>
                        @*<button type="button" class="btn btn-primary" data-dismiss="modal" data-bind="click:$root.addManager" aria-hidden="true">
                            Add
                        </button>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
