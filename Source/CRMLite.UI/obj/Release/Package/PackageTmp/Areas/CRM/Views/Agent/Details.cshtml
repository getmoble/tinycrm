
@{
    ViewBag.Title = "Details";
}
<div class="col-md-12">
    <h3 class="inline-b">Details</h3>
</div>
<input type="hidden" id="hdnAgentId" value="@ViewBag.AgentId" />
<div>
    <div>
        <div class="col-lg-12">
            
                <div class="form-group">
                    <img data-bind="attr:{src:selectedAgent().image}" class="imgsize"/>
                </div>
                <div class="form-group">
                    <label class="lblwidth">First Name</label><span data-bind="text:selectedAgent().firstname"></span>
                </div>
                <div class="form-group">
                    <label class="lblwidth">Last Name</label><span data-bind="text:selectedAgent().lastname"></span>
                </div>
                <div class="form-group">
                    <label class="lblwidth">Phone</label><span data-bind="text:selectedAgent().phoneNumber"></span>
                </div>
                <div class="form-group">
                    <label class="lblwidth">Email</label><span data-bind="text:selectedAgent().email"></span>
                </div>

                <div class="form-group">
                    <label class="lblwidth">Address</label><span data-bind="text:selectedAgent().address"></span>
                </div>
                <div class="form-group">
                    <label class="lblwidth">DED License Number</label><span data-bind="text:selectedAgent().DEDlicenseNumber"></span>
                </div>
                <div class="form-group">
                    <label class="lblwidth">RERA Registration Number</label><span data-bind="text:selectedAgent().RERAregistrationNumber"></span>
                </div>
                <div class="form-group m-l-md">
                    <label class="checkbox-inline i-checks">
                        <input type="checkbox" data-bind="checked:selectedAgent().islistingmember">
                        <i></i>Listing Member
                    </label>

                    @*<input type="checkbox" data-bind="checked:selectedAgent().islistingmember"/><label> Listing Member</label>*@
                </div>
            
        </div>
    </div>
    <div class="clearfix"></div>
</div>
@section scripts{
    <script src="~/App/ViewModels/CRM/models.js"></script>
    <script src="~/App/ViewModels/Agent/AgentViewModel.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var vm = new AgentViewModel();
            ko.applyBindings(vm);
            vm.agentdetails();
        });
    </script>
}
