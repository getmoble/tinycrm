
@{
    ViewBag.Title = "Details";
}
<div class="col-md-12">
<h3 class="inline-b">
    Contact Details</h3>
    <div></div>
        <input type="hidden" id="hdnContactId" value="@ViewBag.ContactId" />
        <div class=" clearfix" data-bind="with:SelectedContact">
            <div class="col-md-6">
                <div class="row">
                    <div class="form-group">
                        <label class="lblwidth"><b>First Name</b>&nbsp;</label>
                        <span data-bind="text:FirstName"></span>
                    </div>
                    <div class="form-group">
                        <label class="lblwidth"><b>Last Name</b>&nbsp;</label>
                        <span data-bind="text:LastName"></span>
                    </div>
                    <div class="form-group">
                        <label class="lblwidth"><b>Title</b>&nbsp;</label>
                        <span data-bind="text:Title"></span>
                    </div>
                    <div class="form-group">
                        <label class="lblwidth"><b>Comapny</b>&nbsp;</label>
                        <span data-bind="text:Comapny"></span>
                    </div>
                    <div class="form-group">
                        <label class="lblwidth"><b>Email</b>&nbsp;</label>
                        <span data-bind="text:Email"></span>
                    </div>
                    <div class="form-group">
                        <label class="lblwidth"><b>Secondary Email</b>&nbsp;</label>
                        <span data-bind="text:SecondaryEmail"></span>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <div class="form-group">
                        <label class="lblwidth"><b>Phone</b>&nbsp;</label>
                        <span data-bind="text:Phone"></span>
                    </div>
                   
                    <div class="form-group">
                        <label class="lblwidth"><b>Addess</b>&nbsp;</label>
                        <span data-bind="text:Address"></span>
                    </div>
                    <div class=" form-group">
                        <label class="lblwidth"><b>State</b>&nbsp;</label>
                        <span data-bind="text:State"></span>
                    </div>
                    <div class=" form-group">
                        <label class="lblwidth"><b>City</b>&nbsp;</label>
                        <span data-bind="text:City"></span>
                    </div>
                    <div class=" form-group">
                        <label class="lblwidth"><b>Region</b>&nbsp;</label>
                        <span data-bind="text:Region"></span>
                    </div>
                    <div class=" form-group">
                        <label class="lblwidth"><b>Description</b>&nbsp;</label>
                        <span data-bind="text:Comments"></span>
                    </div>
                    <div class=" form-group">
                        <label class="lblwidth"><b>Created On</b>&nbsp;</label>
                        <span data-bind="text:CreatedOn"></span>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    @section scripts{
        <script src="~/Content/js/select2.min.js"></script>
        <script src="~/App/ViewModels/CRM/models.js"></script>
        <script src="~/App/ViewModels/Contact/ContactViewModel.js"></script>
        <script type="text/javascript">
            $(document).ready(function () {
                var vm = new ContactViewModel();
                ko.applyBindings(vm);
                vm.contactdetails();
            });
        </script>
    }
